#! /usr/bin/python
#coding:UTF-8
import os
def operation_file(file_name):
    if(os.path.exists(file_name)):
        h_file = None
        try:
            h_file = open(file_name,r+)
        finally:
            h_file.close()
    else:
        return (False,"文件不存在")

if __name__ == '__main__':
    pass